{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Flutter: Run Web App",
            "type": "shell",
            "command": "flutter",
            "args": [
                "run",
                "-d",
                "chrome",
                "--web-renderer",
                "html"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [],
            "presentation": {
                "reveal": "always",
                "panel": "new"
            }
        },
        {
            "label": "Flutter: Clean",
            "type": "shell",
            "command": "flutter",
            "args": [
                "clean"
            ],
            "problemMatcher": []
        },
        {
            "label": "Flutter: Build Web",
            "type": "shell",
            "command": "flutter",
            "args": [
                "build",
                "web",
                "--web-renderer",
                "html"
            ],
            "group": "build",
            "problemMatcher": [],
            "presentation": {
                "reveal": "always",
                "panel": "new"
            }
        },
        {
            "label": "Flutter: Get Dependencies",
            "type": "shell",
            "command": "flutter",
            "args": [
                "pub",
                "get"
            ],
            "problemMatcher": []
        }
    ]
}
